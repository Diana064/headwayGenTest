{
  "compilerOptions": {
    "baseUrl": "src",
    "target": "es5",
    "module": "esnext",
    "lib": ["dom", "dom.iterable", "esnext"],
    "jsx": "react",
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "paths": {
      "*": ["*", "src/*"]
    },
    "resolveJsonModule": true
  },
  "include": ["src"]
}

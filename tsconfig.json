{
  "compilerOptions": {
    "baseUrl": "src",
    "target": "es5",
    "module": "esnext",
    "lib": ["dom", "dom.iterable", "esnext"],
    "jsx": "react",
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "paths": {
      "*": ["*", "src/*"]
    },
    "resolveJsonModule": true
  },
  "include": [
    "src",
    "node_modules/react-loader-spinner",
    "src/components",
    "src/custom.d.ts"
  ]
}
